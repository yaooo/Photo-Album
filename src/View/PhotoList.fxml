<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PhotoListController">
   <children>
      <Button fx:id="copyPhoto1" layoutX="32.0" layoutY="500.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="118.0" text="Copy Photo" />
      <Button fx:id="movePhoto1" layoutX="173.0" layoutY="500.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="118.0" text="Move Photo" />
      <Button fx:id="addPhoto1" layoutX="318.0" layoutY="500.0" mnemonicParsing="false" onAction="#handleAddButton" prefHeight="31.0" prefWidth="118.0" text="Add Photo" />
      <Button fx:id="renamePhoto1" layoutX="474.0" layoutY="500.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="108.0" text="Rename Photo" />
      <Button fx:id="photoTag1" layoutX="608.0" layoutY="500.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="118.0" text="Photo Tag" />
      <Button fx:id="slideShow1" layoutX="757.0" layoutY="500.0" mnemonicParsing="false" prefHeight="31.0" prefWidth="118.0" text="Slide Show" />
      <Button fx:id="exit1" layoutX="366.0" layoutY="555.0" mnemonicParsing="false" onAction="#handleExitButton" prefHeight="31.0" prefWidth="170.0" text="Exit" />
      <Text fx:id="albumTitle1" layoutX="362.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Album title">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <FlowPane layoutX="58.0" layoutY="91.0" prefHeight="372.0" prefWidth="784.0">
         <children>
            <ImageView fx:id="photoList" fitHeight="374.0" fitWidth="786.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </FlowPane>
   </children>
</AnchorPane>
